question link:- https://www.codechef.com/OCT17/problems/CHEFCOUN/
Solution :-
#include<bits/stdc++.h>
using namespace std;
 
int main(){
  int t;
  //cout<<"Enter t\n";
  cin>>t;
  while(t--){
    int n;
    //cout<<"Enter n\n";
    cin>>n;
    int a=100000;
    int b=2;
    int d=1;
    //int c=0;
    //long long int sum=0;
    for(int i=0;i<n;i++){
      if(i<=42948){
        cout<<a<<" ";
        //sum+=a;
      }
      else if(i>42948 && i<52948){
        cout<<b<<" ";
        //sum+=b;
      }
      else{
        cout<<d<<" ";
        //sum+=d;
      }
    }
    //cout<<sum;
    cout<<endl;
  }
} 
